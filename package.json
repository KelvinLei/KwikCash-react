  "main": "client.js",
  "scripts": {
    "start": "npm run clean && npm run build && better-npm-run start-server",
    "dev": "npm run clean && npm run build-server && npm run build-webpack-config && better-npm-run start-server",
    "clean": "rimraf dist build lib",
    "build": "npm run build-server && npm run build-webpack-config && better-npm-run create-app-vendor-bundle",
    "build-server": "babel server -d lib/server",
    "build-webpack-config": "babel webpack -d lib/webpack",
    "test": "mocha --compilers js:babel-core/register --recursive",
    "test:watch": "npm run test -- --watch",
    "postinstall": "npm run build"
  },
  "betterScripts": {
    "create-app-vendor-bundle": {
      "command": "babel-node lib/webpack/compile.js",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "start-server": {
      "command": "node lib/server/server.js",
      "env": {
        "DEBUG": "app:*"
      }
    }
  },
