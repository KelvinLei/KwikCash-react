{
  "presets": ["react", "es2015", "stage-1"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        },
        "transform-decorators-legacy",
        "transform-es2015-template-literals"]
      ]
    }
  }
}
